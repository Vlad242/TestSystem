<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<body>
{% block body %}
    <div class="row">
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
            <a href="{{ path("questions_new") }}" class="btn btn-info">
                <span class="glyphicon glyphicon glyphicon-plus" aria-hidden="true"></span> NEW </a>
            <a href="{{ path("questions_list") }}" class="btn btn-success">
                <span class="glyphicon glyphicon glyphicon-th-list" aria-hidden="true"></span> List </a>
            <a href="{{ path("test_new") }}" class="btn btn-primary">
                <span class="glyphicon glyphicon glyphicon glyphicon-check" aria-hidden="true"></span> Test </a>
        </div>
        <div class="col-sm-2">
            <a href="{{ path("login") }}" class="btn btn-primary">
                <span class="glyphicon glyphicon glyphicon glyphicon-log-in" aria-hidden="true"></span>  LogIn </a>
            <a href="{{ path("logout") }}" class="btn btn-danger">
                <span class="glyphicon glyphicon glyphicon glyphicon-log-out" aria-hidden="true"></span>  LogOut </a>
        </div>
    </div>
{% endblock %}
</body>
</html>
